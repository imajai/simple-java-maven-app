pipeline {
    agent any
    stages {
        stage('Build-dev') {
            steps {
                sh 'sudo touch /root/demo-1.txt'
            }
        }
      stage('Package-dev') {
            steps {
                sh 'echo "file created" '
            }
        }
        stage('post-dev') {
            steps {
                echo "final stage"
            }
        }
    }
}
