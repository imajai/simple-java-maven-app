pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                #sh 'sudo maven compile'
                sh 'sudo touch /root/demo-1.txt'
            }
        }
      stage('Package') {
            steps {
                #sh 'sudo maven package'
                sh 'echo "file created" '
            }
        }
        stage('post') {
            steps {
                echo "final stage"
            }
        }
    }
}
